%table.energy_imports
  %thead
    %tr
      %th= Current.setting.start_year
      %th= Current.setting.end_year
  %tbody
    %tr
      %td#left
        .chart{ :'data-chart_id' => 'waterfall_present_energy_net_imports', 'data-wrapper' => '#left' }
      %td#right
        .chart{ :'data-chart_id' => 'waterfall_future_energy_net_imports', 'data-wrapper' => '#right' }
